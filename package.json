{
  "name": "api-sdk-ts",
  "type": "module",
  "version": "1.0.0",
  "description": "API SDK 抽象",
  "author": "linden",
  "license": "ISC",
  "keywords": [
    "alist",
    "client"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "!src/__tests__",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.2",
    "@types/jest": "^29.5.12",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.ts"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
